<?xml version="1.0" encoding="utf-8"?>
<AutoVisualizer xmlns="http://schemas.microsoft.com/vstudio/debugger/natvis/2010">
  
  <Type Name="MeasureCol2D">
    <DisplayString ExcludeView="nop,verbose">{{{*Meas.X.total,g},{*Meas.Y.total,g}} {{p{{{*Meas.X.p_total,g},{*Meas.Y.p_total,g}}}}</DisplayString>
    <DisplayString IncludeView="verbose">{{{Meas.X,g},{Meas.Y,g},saved{saved}}}</DisplayString>
    <DisplayString IncludeView="nop">{{{*Meas.X.total,g},{*Meas.Y.total,g}}</DisplayString>
    <Expand HideRawView="true">
      <Item Name="Meas">Meas</Item>
      <Item Name="saved">saved,view(simple)</Item>
    </Expand>
  </Type>
  <Type Name="MeasureCol">
    <DisplayString>{{me={*total,g},p={*p_total,g}}</DisplayString>
    <DisplayString Condition="measures.size() == 0">Empty</DisplayString>
    <Expand HideRawView="true">
      <Item Name="saved" ExcludeView="simple">saved,view(simple)</Item>
      <Item Name="[size]" ExcludeView="simple">measures.size()</Item>
      <CustomListItems>
        <Variable Name="i" InitialValue="0" />
        <Size>measures.size()</Size>
        <Loop>
          <Item>measures[i]</Item>
          <Exec>i++</Exec>
        </Loop>
      </CustomListItems>
    </Expand>
  </Type>
  <Type Name="glm::vec&lt;4,float,0&gt;">
  <DisplayString>{{{x,g},{y,g},{z,g},{w,g}}}</DisplayString>
    <Expand HideRawView="true">
      <Item Name="x">x,g</Item>
      <Item Name="y">y,g</Item>
      <Item Name="z">z,g</Item>
      <Item Name="w">w,g</Item>
    </Expand>
</Type>
  <Type Name="glm::vec&lt;3,float,0&gt;">
    <DisplayString>{{{x,g},{y,g},{z,g}}}</DisplayString>
    <Expand HideRawView="true">
      <Item Name="x">x,g</Item>
      <Item Name="y">y,g</Item>
      <Item Name="z">z,g</Item>
    </Expand>
  </Type>
  <Type Name="DrawObj2D" IncludeView="original">
    <DisplayString>{{{Name}}</DisplayString>
  </Type>
  <Type Name="DrawObj2D" Inheritable="true" ExcludeView="original">
    <DisplayString IncludeView="ssimple">{{{Name}}</DisplayString>
    <DisplayString ExcludeView="raw">{{N{Name},P{*Parent,view(ssimple)},cng={States.Changed},AS{ActualSize},AP{ActualPos},LS{{{*LogicalSize.X,g}}{{{*LogicalSize.Y,g}}},S{Size},P{Pos,view(nop)},{Childs}}</DisplayString>
    <Expand HideRawView="true">
      <Item Name="Name">Name</Item>
      <Item Name="this(original)" ExcludeView="original">this,view(original)</Item>
      <Item Name="Align">(Align_S*)this</Item>
      <Item Name="ActualPos">ActualPos</Item>
      <Item Name="ActualSize">ActualSize</Item>
      <Item Name="GUID">GUID</Item>
      <Item Name="Childs">Childs,view(simple)</Item>
      <Item Name="Pos">Pos</Item>
      <Item Name="Size">Size</Item>
      <Item Name="m_Bounds">m_Bounds</Item>
      <Item Name="TexSize">TexSize</Item>
      <Item Name="Linked">Linked</Item>
      <Item Name="Changed">States.Changed</Item>
      <Item Name="Parent">Parent</Item>
      <Item Name="Visibility">Visibility</Item>
    </Expand>
  </Type>
  <Type Name="StackPanel" Inheritable="true" ExcludeView="original">
