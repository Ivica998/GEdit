  <Type Name="Bounds">
    <DisplayString>{{{x1,g},{y1,g},{x2,g},{y2,g}}}</DisplayString>
    <Expand HideRawView="true">
      <Item Name="x1">x1</Item>
      <Item Name="y1">y1</Item>
      <Item Name="x2">x2</Item>
