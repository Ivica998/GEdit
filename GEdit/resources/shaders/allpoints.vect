#version 330 core
layout (location = 0) in vec3 aPos;


uniform mat4 projection;
uniform mat4 view;
//uniform mat4 model;

out VS_OUT {
    vec3 color;
} vs_out;

void main()
{
    vs_out.color = vec3(0,1,1);
    gl_Position = projection * view * vec4(aPos, 1.0);
    //gl_Position = vec4(aPos, 1.0);
}